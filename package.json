{
  "name": "seulgiryuhun-components",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "homepage": "http://seulgiryuhun.github.io/seulgiryuhun",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
