{
  "name": "seulgiryuhun-components",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "version": "0.0.10",
  "type": "module",
  "homepage": "http://seulgiryuhun.github.io/seulgiryuhun",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint-staged": "lint-staged",
    "preview": "vite preview",
    "test:commit": "vitest --watch=false",
    "test": "vitest",
    "sb": "storybook dev -p 6006",
    "build:storybook": "storybook build -o dist/storybook",
    "deploy:storybook": "touch ./dist/storybook/.nojekyll && gh-pages -d dist/storybook -t true",
    "predeploy": "pnpm run build:storybook",
    "publish:npm": "rm -rf dist && mkdir dist && babel src/lib -d dist --copy-files",
    "format": "prettier --cache --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^16.2.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.14",
    "@storybook/react": "^7.6.14",
    "@storybook/react-vite": "^7.6.14",
    "@storybook/test": "^7.6.14",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.11.21",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.6.15",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "path": "^0.12.7",
    "prettier": "^3.2.5",
    "sass": "^1.70.0",
    "storybook": "^7.6.14",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vite-plugin-dts": "^3.7.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.3.1"
  }
}
